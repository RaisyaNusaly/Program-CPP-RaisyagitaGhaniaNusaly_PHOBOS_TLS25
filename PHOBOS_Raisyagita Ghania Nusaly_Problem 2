#include <iostream>
#include <string>
using namespace std;

bool isVowel(char c) {
    return c=='a'||c=='e'||c=='i'||c=='o'||c=='u'||
           c=='A'||c=='E'||c=='I'||c=='O'||c=='U';
}

// manual reverse (no std::reverse)
void manualReverse(string &s) {
    int i = 0, j = (int)s.size() - 1;
    while (i < j) {
        char t = s[i];
        s[i] = s[j];
        s[j] = t;
        i++; j--;
    }
}

int main() {
    string word;
    if (!getline(cin, word)) return 0;

    string pwd;
    for (size_t i = 0; i < word.size(); ++i) {
        char c = word[i];
        if (isVowel(c)) {
            int ascii = (int)c;       
            pwd += to_string(ascii);     
        } else {
            pwd += c;                      
        }
    }

    manualReverse(pwd);                    
    cout << pwd << "\n";
    return 0;
}
